{
  "manifest_version": 2,

  "name": "Session  Saver",
  "description": "This extension saves the current session of tabs on window",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon2.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage"
  ]
}
